type Query {
    articles: [Article!]!
}

type Article {
    id: String!
    typ: String!
    date: String!
    title: String!
    html: String!
    images: [ImgMeta!]!
    dokuments: [Dokument!]!
}

type ImgMeta {
    id: String!
    url: String!
    alt: String!
}

type Dokument {
    id: String!
    title: String!
    url: String!
}
